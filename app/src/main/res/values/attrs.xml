<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StorageBarView">
        <attr name="usageFraction" format="float" />
        <attr name="usedText" format="string" />
        <attr name="totalText" format="string" />
        <attr name="unitText" format="string" />
    </declare-styleable>

    <declare-styleable name="ProfileInfoView">
        <attr name="profileName" format="string" />
        <attr name="profileEmail" format="string" />
        <attr name="profileImage" format="reference" />
    </declare-styleable>
    <declare-styleable name="AlbumMetaInfoView">
        <attr name="capturedCount" format="integer" />
        <attr name="recordedCount" format="integer" />
        <attr name="loading" format="boolean" />
    </declare-styleable>

</resources>

